<h1>Add a new bookmark</h1>
<div class="container">
<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie, collection: Movie.all%>
  <div class="button">
    <%= f.submit "Add bookmark", class: "btn btn-primary" %>
    <%= link_to "sommaire", lists_path, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
